public class Solution {
    public int reverse(int x) {
         /**
     * 考虑溢出
     * + - 号不用考虑
     * */

     double res = 0;
     while(x != 0){
          //int single = x % 10;
         res = res*10 + x % 10;
         x = x/10;
     }

     if(res > Integer.MAX_VALUE || res < Integer.MIN_VALUE)
         res = 0;

     return (int) res;
    }
}
