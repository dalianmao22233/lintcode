public class Solution {
    public int climbStairs(int n) {
        if(n <= 1){
            return 1;
        }
        int now = 0, lastlast = 1, last = 1;
        for (int i = 2; i <= n; i++){
            now = lastlast+last;
            lastlast = last;
            last = now;
        }
        return now;
    }
}
