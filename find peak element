public class Solution {
    public int findPeakElement(int[] nums) {
        if(nums == null|| nums.length == 0) return -1;
        if(nums.length == 1) return 0;
        int low = 0, mid = 0, high = nums.length - 1;
        while(low < high) {
            mid = low + (high-low)/2;
            if(nums[mid] < nums[mid+1]) low = mid+1;
            else high = mid;
        }
        return low;
 
    }
}
